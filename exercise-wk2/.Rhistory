DescribeDifference <- function(n, m) {
if(n>m) {
return(paste("Your first vector is longer by", length(n>m), "elements"))
} else {
return(paste("Your second vector is longer by", length(m>n), "elements"))
}
}
# Pass two vectors to your `DescribeDifference` function
long <- c(1, 2, 3, 4, 5, 6)
short <- c(1, 2)
DescribeDifference(long, short)
